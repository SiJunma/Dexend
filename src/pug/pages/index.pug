extends ../layout/layout.pug

block header
  header.header
    .header__offer-wrapper
      ul.header__links
        li.header__link
          a(href='#')
            | Behance
            img(src='img/arrow_header-link.svg' alt='arrow_header-link')
        li.header__link
          a(href='#')
            | Dribbble
            img(src='img/arrow_header-link.svg' alt='arrow_header-link')
        li.header__link
          a(href='#')
            | Facebook
            img(src='img/arrow_header-link.svg' alt='arrow_header-link')
        li.header__link
          a(href='#')
            | Twitter
            img(src='img/arrow_header-link.svg' alt='arrow_header-link')
        li.header__link
          a(href='#')
            | Medium
            img(src='img/arrow_header-link.svg' alt='arrow_header-link')
        li.header__link
          a(href='#')
            | Linkedin
            img(src='img/arrow_header-link.svg' alt='arrow_header-link')


      .header__offer
        h1.header__offer-title
          | We Create Digital <br> 
          span Products
        a.btn.header__offer-btn(href='#')
          | Submit Request

      .header__clickme-wrapper
        a.header__clickme-button(href='#')
          img(src='img/more_button.svg' alt='Show')
        img.header__clickme-bg(src='img/more-projects.svg' alt='')
        img.header__clickme-arrow(src='img/click-me_button.svg' alt='Click me')
    .header__media
      .header__media-bg 
        img(src="img/header__media-bg.png")
      .header__media-river 
        img(src="img/header__media-river.png")



