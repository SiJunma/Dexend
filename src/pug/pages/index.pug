            li Dashboard
            li Branding
            li Mobile

          img(src="img/project-img5.jpg", alt="Project image").projects__card-img
        
        a.projects__card(href='#')
          h3.projects__card-title Homebud
          ul.projects__card-tags 
            li Dashboard
            li Branding
            li Mobile

          img(src="img/project-img6.jpg", alt="Project image").projects__card-img
        
        a.projects__card(href='#')
          h3.projects__card-title Delivery Food
          ul.projects__card-tags 
            li Dashboard
            li Branding
            li Mobile

          img(src="img/project-img7.jpg", alt="Project image").projects__card-img
        
        .projects__card.projects__card_white
          .projects-links
            .projects-links__col
              .projects-links__more
                a.btn.btn-circle(href='#')
                h3.projects-links__title More <br> projects
            .projects-links__col
              .projects-links__category
